<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin.ru.own.www.mybatis.dao.Sku_AtrValueDAO">
	
	<insert id="insert" parameterType="admin.ru.own.www.entity.SKU_AtrValue">
		insert into sku_atrvalue (attr_value_id, sku_id)
		values (#{attr_value_id},#{sku_id})
	</insert>
	
	<delete id="delete" parameterType="int">
    	delete from own.sku_atrvalue where sku_id=#{sku_id}    
    </delete>
    
</mapper>